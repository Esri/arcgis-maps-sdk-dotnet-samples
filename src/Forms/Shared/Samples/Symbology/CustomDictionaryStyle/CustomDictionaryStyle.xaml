<ContentPage x:Class="ArcGISRuntimeXamarin.Samples.CustomDictionaryStyle.CustomDictionaryStyle"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:esriUI="clr-namespace:Esri.ArcGISRuntime.Xamarin.Forms;assembly=Esri.ArcGISRuntime.Xamarin.Forms">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <esriUI:MapView x:Name="MyMapView" Grid.Row="0" />
        <Label Grid.Row="1"
               Padding="0,0,0,5"
               Text="Select Dictionary Symbol Style Source" />
        <Button x:Name="LocalStyleButton"
                Grid.Row="2"
                Clicked="LocalStyleButtonClicked"
                IsEnabled="False"
                Text="Local .stylx file" />
        <Button x:Name="WebStyleButton"
                Grid.Row="3"
                Clicked="WebStyleButtonClicked"
                Text="Web style" />
    </Grid>
</ContentPage>
