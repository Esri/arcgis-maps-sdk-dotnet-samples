<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ArcGISMapsSDK.SampleListPage">
        <ListView x:Name="SampleList" ItemsSource="{Binding .}" ItemTapped="OnItemTapped" SelectionMode="None">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding SampleName}" TextColor="{AppThemeBinding Light=Black, Dark=LightGray}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
</ContentPage>